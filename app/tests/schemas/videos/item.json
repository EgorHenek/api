{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": [
    "title",
    "slug",
    "date",
    "yt_id",
    "yt_thumbnail",
    "duration",
    "channel_id",
    "liked",
    "id"
  ],
  "additionalProperties": false,
  "properties": {
    "title": {
      "$id": "#/properties/title",
      "type": "string"
    },
    "slug": {
      "$id": "#/properties/slug",
      "type": "string"
    },
    "date": {
      "$id": "#/properties/date",
      "type": [
        "string",
        "null"
      ]
    },
    "yt_id": {
      "$id": "#/properties/yt_id",
      "type": "string"
    },
    "yt_thumbnail": {
      "$id": "#/properties/yt_thumbnail",
      "type": "string"
    },
    "duration": {
      "$id": "#/properties/duration",
      "type": "integer"
    },
    "channel_id": {
      "$id": "#/properties/channel_id",
      "type": [
        "integer",
        "null"
      ]
    },
    "liked": {
      "$id": "#/properties/liked",
      "type": "boolean"
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer"
    }
  }
}