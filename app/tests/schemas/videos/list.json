{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "array",
  "additionalItems": false,
  "minItems": 1,
  "items": {
    "$id": "#/items",
    "type": "object",
    "required": [
      "title",
      "slug",
      "date",
      "yt_id",
      "yt_thumbnail",
      "duration",
      "channel_id",
      "liked",
      "id"
    ],
    "additionalProperties": false,
    "properties": {
      "title": {
        "$id": "#/items/properties/title",
        "type": "string"
      },
      "slug": {
        "$id": "#/items/properties/slug",
        "type": "string"
      },
      "date": {
        "$id": "#/items/properties/date",
        "type": [
          "string",
          "null"
        ]
      },
      "yt_id": {
        "$id": "#/items/properties/yt_id",
        "type": "string"
      },
      "yt_thumbnail": {
        "$id": "#/items/properties/yt_thumbnail",
        "type": "string"
      },
      "duration": {
        "$id": "#/items/properties/duration",
        "type": "integer"
      },
      "channel_id": {
        "$id": "#/items/properties/channel_id",
        "type": [
          "integer",
          "null"
        ]
      },
      "liked": {
        "$id": "#/items/properties/liked",
        "type": "boolean"
      },
      "id": {
        "$id": "#/items/properties/id",
        "type": "integer"
      }
    }
  }
}